{
    "version":1,
    "disable_existing_loggers":false,
    "formatters":{
        "fmt1":{
            "format":"%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers":{
        "hand1":{
            "class":"logging.StreamHandler",
            "level":"DEBUG",
            "formatter":"fmt1",
            "stream":"sys.stdout"
        }
    },
    "root":{
        "level":"DEBUG",
        "handlers":["hand1"]
    }
}
